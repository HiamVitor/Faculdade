	<queryString language="SQL">
		<![CDATA[SELECT v.id,
	   v.valor,	   
	   v.id_cliente,
	   v.id_pagamento,
	   v.id_veiculo,
	   v.id_vendedor,
	   c.nome as nome_cliente,
	   p.nome as forma_pagamento,
	   vend.nome as nome_vendedor,
	   vei.nome as nome_veiculo,
	   (SELECT sum(v.valor)
		 FROM VENDAS v) as valor_total
FROM vendas v
JOIN clientes c on (c.id = v.id_cliente)
JOIN pagamentos p on (p.id = v.id_pagamento)
JOIN vendedores vend on (vend.id = v.id_vendedor)
JOIN veiculos vei on (vei.id = v.id_veiculo)]]>
	</queryString>
